interface Animal
{
	void Sound(String x);
  
	default void normalFun()
	{
	System.out.println("Hello");
	}
}

class Dog implements Animal{
	public void Sound(String x){
    System.out.println(x);
    }
}


class Test
{
	public static void main(String args[])
	{
		Dog na = new Dog();
		na.Sound("Bark");
	}
}
